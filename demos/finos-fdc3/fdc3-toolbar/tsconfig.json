{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "pretty": true,
    "outDir": "app-js"
  },
  "include": [
    "app/main.ts",
    "app/preload.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
